<!DOCTYPE html>
<html>
<head>
    <%- include('../app-general/partials/admin-head.ejs') %>
</head>
<body>
    <%- include('../app-general/partials/app-horizontal-nav.ejs') %>
    <div class='admin-body'>
            <div class='left-section'>
                <%- include('../app-general/partials/admin-nav.ejs') %>
            </div>
            <div class='mid-section'>
                <%- include('./partials/admin-teacher-region-mid-nav.ejs', {data}) %>
                <table>
                <tr>
                    <th>Teacher's Name</th>
                    <th>DOB</th>
                    <th>Age Group</th>
                    <th>Teacher's Contact</th>
                </tr>
                <% data.forEach(datum => { %>
                <tr>
                    <td><%= datum.formatChildFullName() %></td>
                        <td><%= datum.dob %></td>
                        <td><%= datum.ageGroup %></td>
                        <td><%= datum.formatTeacherNumber() %></td>
                </tr>
                <% }) %>
                </table>
            </div>
            <div class='right-section'>
            </div>
        </div>
</body>
</html>